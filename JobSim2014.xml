<stdout URL="file:/gpfs01/star/pwg/lomicond/Ondrej/Jets/D0JetAngularities/out/$JOBID.out" />
<stderr URL="file:/gpfs01/star/pwg/lomicond/Ondrej/Jets/D0JetAngularities/err/$JOBID.err" />

                
<Generator>
<Location>/gpfs01/star/pwg/lomicond/Ondrej/Jets/D0JetAngularities/report/</Location>
<ScriptLocation>/gpfs01/star/pwg/lomicond/Ondrej/Jets/D0JetAngularities/csh/</ScriptLocation>
<ListLocation>/gpfs01/star/pwg/lomicond/Ondrej/Jets/D0JetAngularities/list/</ListLocation>
</Generator>
                                                
<command>
	starver SL22c

	setenv FASTJET /gpfs01/star/pwg/lomicond/Ondrej/Jets/Alex_install/install/fastjet-install
	set MCFILENAME = &quot;./mcEventLists/embedding2014.list&quot;

	shuf -n 50 ${MCFILENAME} &gt; ${JOBID}_${JOBINDEX}_MC.list
	cp `cat ${JOBID}_${JOBINDEX}_MC.list` .
	ls rcf*.root &gt; ${JOBID}_${JOBINDEX}_MC_Complete.list

	root4star -l -b -q runD0JetSim.C\(\&quot;$FILELIST\&quot;,\&quot;${JOBID}_${JOBINDEX}_MC_Complete.list\&quot;,\&quot;MC_2014_MidLow_${JOBINDEX}\&quot;,100000000,false\) &gt; MC_2014_MidLow_${JOBINDEX}.log
	date
</command>

<output fromScratch="MC_2014_MidLow_*.root" toURL="/gpfs01/star/pwg/lomicond/Ondrej/Jets/D0JetAngularities/production/" />
<output fromScratch="*.log" toURL="/gpfs01/star/pwg/lomicond/Ondrej/Jets/D0JetAngularities/log/" />
<SandBox installer="ZIP">
	<Package name="LocalLibraries">
	<File>file:./StRoot/</File>
	<File>file:./fastjet</File>
	<File>file:./siscone</File>
	<File>file:./.sl73_gcc485/</File>
	<File>file:./runD0JetSim.C</File>
	<File>file:./mcEventLists/</File>
	</Package>
</SandBox>

</job>
